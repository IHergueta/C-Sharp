            @media screen and (max-width: 1050px){
                .container, .pagination-inner, .note-container{
                    width: 510px !important;
                    }
                }
                
            /* Basics */
            
            a{
                transition: all 0.3s ease-in-out; 
                color: #000;
                text-decoration: none;
                }
            p{
                margin: 20px 0 0 0;
                padding: 0;
                color: #FFF;
                }
            p:first-child{
                margin: 0;
                }
            img{
                display: block;
                margin: 0;
                padding: 0;
                }
            ul, ol{
                padding: 0;
                margin: 0;
                }
            #none{
                display: none;
                }
            blockquote{
                margin: 20px 20px 0px 20px;
                }
            h3{
                padding: 0;
                margin: 0;
                margin-bottom: 20px;
                font-family: 'Roboto';
                font-weight: 300;
                font-size: 30px;
                line-height: 40px;
                }
            h3 a{
                color: #FFF !important;
                border-bottom: 1px solid #FFF !important;
                text-decoration: none;
                padding: 0;
                border: 0;
                }
                
            /* Posts */
                
            .post{
                line-height: 1.7em;
                border: 1px solid #FFF;
                padding: 20px;
                box-sizing: border-box;
                background-color:rgba(255,255,255,0.1);
                
                margin: 0px 10px 20px 10px;
                width: 490px;
                float: left;
                position: relative; 
                
                
                }
            .post a{
                padding-bottom: 2px;
                border-bottom: 1px solid #000;
                }
            .post a:hover{
                color:#ffb700;
                border-bottom: 1px solid #ffb700;
                }
            .post img{
                width: 100%;
                }
            .post ul li{
                margin-left: 30px;
                }
            .meta{
                width: 100%;
                margin-top: 20px;
                padding-top: 20px;
                border-top: 1px solid #FFF;
                float:left;
                }
            .reblogbutton, .likebutton{
                height: 40px;
                width: 40px;
                line-height: 40px;
                overflow: hidden;
                float: left;
                border: 1px solid #FFF;
                box-sizing: border-box;
                transition: all 0.3s ease-in-out;  
                margin-right: 20px;
                text-align: center;
                }
            .reblogbutton a{
                padding: 0px 11px;
                border: 0;
                }
            .permalink{
                float: right;
                font-size: 20px;
                height: 40px;
                line-height: 38px;
                width: 40px;
                text-align: center;
                color: #FFF !important;
                padding: 0px !important;
                border: 1px solid #FFF !important;
                box-sizing: border-box;
                }
            .permalink:hover, .notecount:hover{
                color: #FFF;
                border: 0;
                background-color: #ffb700;
                }
            .notecount{
                float: right;
                font-size: 12px;
                height: 40px;
                line-height: 40px;
                padding: 0px 14px !important;
                text-align: center;
                border: 1px solid #FFF !important;
                color: #FFF !important;
                border: 0;
                text-decoration: none;
                font-family: 'Roboto';
                font-weight: 300;
                letter-spacing: 1px;
                text-transform: uppercase;
                }
            .reblogbutton:hover, .likebutton:hover{
                background-color: #ffb700;
                cursor:pointer;
                }
            .credit{
                width: 100%;
                overflow: hidden;
                font-size: 11px;
                font-family: 'Roboto';
                color: #FFF;
                font-weight: 300;
                box-sizing: border-box;
                padding: 30px 40px 30px 30px;
                display: block;
                text-align: right;
                letter-spacing: 1px;
                margin: 0;
                }
            .credit a{
                color: #FFF;
                text-decoration: none;
                padding-bottom: 2px;
                border-bottom: 1px solid #FFF;
                }
            .tag-container{
                text-align: justify;
                margin-top: 20px;
                padding-top: 20px;
                width: 100%;
                border-top: 1px solid #FFF;
                }   
            .caption{
                text-align: justify;
                
                margin-top: 20px;
                
                
                width: 100%;
                }
            /* Text Posts */

            .post.text p{
                text-align: justify;
                }
                
            /* Photo Posts */
            
            /* Photoset Posts */
            
            ul#photoset{
                list-style-type: none;
                padding: 0;
                margin: 0;
                }
            ul#photoset li{
                padding: 0;
                margin: 0px 0px 10px 0px;
                }
            ul#photoset li:last-child { 
                margin-bottom: 0px;        
                }
                
            /* Video Posts */
            
            .videoWrapper {
                position: relative;
                padding-bottom: 56.25%; /* 16:9 */
                padding-top: 25px;
                height: 0;
                }
            .videoWrapper iframe {
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                }
            
            /* Audio Posts */
                
            /* Quote Posts */
            
            .post.quote .quote{
                width: 100%;
                overflow: hidden;
                font-family: 'Playfair Display';
                font-style: italic;
                color: #FFF;
                font-size: 30px;
                margin-bottom: 20px;
                float:left;
                line-height: 40px;
                }
            .post.quote .source{
                width: 100%;
                overflow: hidden;
                font-family: 'Roboto';
                font-weight: 300;
                text-transform: uppercase;
                letter-spacing: 1px;
                font-size: 12px;
                color: #FFF;
                text-align: right;
                }
            
            /* Link Posts */

            .post.link a.link{
                display: block;
                border: 1px solid #FFF;
                color: #FFF;
                text-decoration: none;
                }
            .post.link a.link:hover{
                background-color:#ffb700;
                }
            .post.link .link-thumb img{
                width: 100%;
                height: auto;
                }
            .post.link .link-host{
                margin: 0px 20px;
                padding: 20px 0px;
                border-bottom: 1px solid #FFF;
                font-family: 'Roboto';
                text-transform: uppercase;
                font-weight: 300;
                letter-spacing: 1px;
                box-sizing: border-box;
                line-height: 15px;
                }
            .post.link .link-title{
                font-family: 'Roboto';
                text-transform: uppercase;
                font-weight: 300;
                letter-spacing: 1px;
                font-size: 30px;
                line-height: 30px;
                padding: 20px;
                }
            
            /* Chat Posts */
            
            .post.chat ul{
                padding: 0; 
                margin: 0;
                }
            .post.chat ul li{
                list-style-type: none;
                padding: 20px 0 0 0;
                margin: 20px 0 0 0;
                border-top: 1px solid #FFF;
                }
            .post.chat ul li:first-child { 
                padding: 0;
                margin: 0;
                border-top: 0px;      
                }
            .post.chat ul li span.label{
                font-family: 'Roboto';
                font-weight: 300;
                text-transform: uppercase;
                letter-spacing: 1px;
                }
            
            /* Answer Posts */
            
            .question-container, .answer-container, .replies{
                width: 100%;
                overflow: hidden;
                }
            .question-container{
                border-bottom: 1px solid #FFF;
                margin-bottom: 20px;
                padding-bottom: 20px;
                }
            .question-icon, .answer-icon{
                width: 60px;
                float: left;
                margin-right: 20px;
                }
            .question-icon img, .answer-icon img{
                width: 100%;
                height: auto;
                border-radius: 30px;
                }
            .post.answer .asker, .post.answer .answerer{
                font-family: 'Roboto';
                font-weight: 300;
                text-transform: uppercase;
                letter-spacing: 1px;
                margin-bottom: 10px;
                }
            .post.answer .asker a, .post.answer .answerer a{
                color: #FFF !important;
                border-bottom: 1px solid #FFF !important;
                text-decoration: none;
                }
                
            /* Pagination */
            
            .pagination{
                width: 100%;
                background-color:rgba(255,255,255,0.1);
                overflow: hidden;
                border-top: 1px solid #FFF;
                border-bottom: 1px solid #FFF;
                padding: 30px 0px;
                
                }
            .pagination-inner{
                width: 1530px;
                margin: 0 auto;
                
                }
            .pagination-inner a{
                padding: 10px;
                width: 80px;
                font-size: 12px;
                text-align: center;
                border: 1px solid #FFF;
                color: #FFF;
                text-decoration: none;
                font-family: 'Roboto';
                font-weight: 300;
                text-transform: uppercase;
                letter-spacing: 1px;
                }
            .pagination a#next{
                float: left;
                }
            .pagination a#prev{
                float: right;
                }
            .pagination a:hover{
                background-color: #ffb700;
               }

            /* Notes */

            #note-page{
                width: 100%;
                overflow: hidden;
                padding: 30px 0px;
                font-family: 'Roboto';
                font-weight: 300;
                letter-spacing: 1px;
                }
            .note-container{     
                width: 860px;
                margin: 0 auto;  
                }  
                ol.notes {
                    list-style-type: none;
                    font-weight: 300;
                }
                ol.notes li.note {
                    padding: 5px 0px;
                    font-size: 12px;
                }
                ol.notes li.note a{
                    color: #FFF;
                    padding-bottom: 2px;
                    border-bottom: 1px solid #FFF;
                    text-decoration: none;
                }
                ol.notes li.note a:hover{
                    color: #ffb700;
                    border-bottom: 1px solid #ffb700;
                }
                ol.notes li.note img.avatar {
                    display: none;
                }
                ol.notes li.note span.action {
                    font-weight: normal;
                }
                ol.notes li.note .answer_content {
                    font-weight: normal;
                }
                ol.notes li.note blockquote a {
                    text-decoration: none;
                }
            
            .note.more_notes_link_container{
                    border-top: 1px solid #FFF;
                    padding-top: 20px !important;
                    margin-top: 20px;
                    text-align: left !important;
                   font-size: 14pt !important;
                }
      		.note.more_notes_link_container a, a.more_notes_link, .notes_loading{
        		text-decoration: none;
        		padding: 0 !important;
       			border: 0 !important;
         		color: #FFF !important;
                text-transform: uppercase;
             	font-weight: 300;
                font-family: 'Roboto';
             	text-align: left !important;
               	font-size: 10pt !important;
                }
            #infscr-loading{
                position: fixed;
                z-index: 198;
                bottom: 40px;
                left: 40px;
                width: 20px;
                font-size: 20px;
                margin: 0 auto;
                color: #FFF !important;
                box-sizing: border-box;
                padding: 0px;
                margin: 0;
                }
            #infscr-loading img{
                width: 0px;
                height: 0px;
                float: left;
                }
            #infscr-loading p{
                padding: 0;
                margin: 0;
                }
            
            
                   
